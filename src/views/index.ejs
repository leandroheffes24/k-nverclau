                <i class="far fa-heart text-dark"></i>
            </div>
            <div class="row justify-content-center">
                <div class="col-lg-8">
                    <div class="text-center">
                        <form action="https://formsubmit.co/<%= formMail %>" method="POST">
                            <div class="form-row">
                                <div class="form-group col-sm-6">
                                    <input type="text" class="form-control bg-secondary border-0 py-4 px-3" placeholder="Nombre" maxlength="30" name="nombre"/>
                                </div>
                                <div class="form-group col-sm-6">
