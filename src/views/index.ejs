                                    <input type="email" class="form-control bg-secondary border-0 py-4 px-3" placeholder="Mail" maxlength="50" name="correo electrónico"/>
                                </div>
                                <div class="form-group col-sm-6">
                                    <input type="text" class="form-control bg-secondary border-0 py-4 px-3" placeholder="Número de teléfono" maxlength="25" name="número de contacto"/>
                                </div>
                            </div>
                            <div class="form-group">
                                <textarea class="form-control bg-secondary border-0 py-2 px-3" rows="5" placeholder="Tu mensaje" required="required" maxlength="500" name="mensaje"></textarea>
                            </div>
                            <div>
                                <button class="btn btn-primary font-weight-bold py-3 px-5" type="submit">Enviar</button>
                            </div>
                            <input type="hidden" name="_next" value="<%= domain %>">
                            <input type="hidden" name="_captcha" value="false">
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
